{
  "name": "batList - watchList extension for YouTube",
  "description": "WatchList extension for YouTube",
  "version": "1.01",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open batList"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [ 
    "tabs",
    "notifications"
 ],
  "icons": {
    "16": "logo512.png",
    "48": "logo512.png",
    "128": "logo512.png"
  }


}